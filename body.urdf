<robot name = "robot">
    <link name="Torso">
        <inertial>
            <origin xyz="0 0 2" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0 0 2" rpy="0 0 0"/>
            <geometry>
                <sphere radius="1.5" />
            </geometry>
            <material name="Cyan">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 2" rpy="0 0 0"/>
            <geometry>
                <sphere radius="1.5" />
            </geometry>
        </collision>
    </link>
    <joint name="Torso_RightLeg" type="fixed">
       <parent link="Torso"/>
       <child  link="RightLeg"/>
       <origin rpy="0 0 0" xyz="0.5 0 1" />
     <axis xyz="0 1 0"/>
       <limit effort="0.0" lower="-3.14159" upper="3.14159" velocity="0.0"/>
    </joint>
    <link name="RightLeg">
        <inertial>
            <origin xyz="1 0 1" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="1 0 1" rpy="0 0 0"/>
            <geometry>
                <sphere radius="1.5" />
            </geometry>
            <material name="Cyan">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="1 0 1" rpy="0 0 0"/>
            <geometry>
                <sphere radius="1.5" />
            </geometry>
        </collision>
    </link>
    <joint name="RightLeg_RightLeg2" type="fixed">
       <parent link="RightLeg"/>
       <child  link="RightLeg2"/>
       <origin rpy="0 0 0" xyz="1.5 0 1" />
     <axis xyz="0 1 0"/>
       <limit effort="0.0" lower="-3.14159" upper="3.14159" velocity="0.0"/>
    </joint>
    <link name="RightLeg2">
        <inertial>
            <origin xyz="1.2 0 0" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="1.2 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="1.5" />
            </geometry>
            <material name="Cyan">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="1.2 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="1.5" />
            </geometry>
        </collision>
    </link>
</robot>